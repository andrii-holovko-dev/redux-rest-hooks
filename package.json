{
  "name": "redux-rest-hooks",
  "version": "1.0.1",
  "description": "Declarative REST API fetching using React Hooks and Redux",
  "author": "Andrii Holovko andrii.holovko.dev@gmail.com",
  "license": "MIT",
  "repository": "andrii-holovko-dev/redux-rest-hooks",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepublish": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "dependencies": {
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "lodash": "^4.17.15",
    "microbundle-crl": "^0.13.8",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "^3.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "files": [
    "dist"
  ]
}
